<!--Slick Slider Scripts for Belive-Us Section 6-->
<script type="text/javascript">
  $(".fade-img").slick({
    infinite: true,
    speed: 700,
    autoplay: true,
    autoplaySpeed: 2000,
    fade: true,
    arrows: false,
    slidesToShow: 1,
    slidesToScroll: 1,
    asNavFor: ".fade-txt",
  });

  $(".fade-txt").slick({
    infinite: true,
    speed: 700,
    autoplay: true,
    autoplaySpeed: 2000,
    asNavFor: ".fade-img",
    arrows: true,
    slidesToShow: 1,
    slidesToScroll: 1,
    focusOnSelect: true,
  });
</script>

